//update attendance...

UPDATE Attendance 
SET Status = 'Absent' 
WHERE Stu_id IN ('TG_2021_1234', 'TG_2020_1235', 'TG_2020_1200', 'TG_2021_1256', 'TG_2021_1321', 'TG_2020_1122', 'TG_2021_1403');

//-- change attendance status to absent in who submit medicals

UPDATE Attendance
	SET status='Absent'
	WHERE 
	(stu_id='TG_2020_1200' AND Course_code='ICT1212' AND Date='2024-11-19')OR
	(stu_id='TG_2020_1200' AND Course_code='ICT1222' AND Date='2024-11-19')OR
	(stu_id='TG_2022_1403' AND Course_code='ICT1242' AND Date='2024-08-15')OR
	(stu_id='TG_2022_1403' AND Course_code='ICT1242' AND Date='2024-08-16')OR
	(stu_id='TG_2022_1401' AND Course_code='TCS1212' AND Date='2024-11-19')OR
	(stu_id='TG_2021_1256' AND Course_code='TCS1233' AND Date='2024-09-16')OR
	(stu_id='TG_2021_1256' AND Course_code='TCS1233' AND Date='2024-09-18');
 
//– - change attendance remove mor not eligible

UPDATE attendance
SET status='Present'
WHERE Stu_id='TG_2022_1403' and Course_code='ENG1222' or Course_code='ICT1242'or Course_code='ICT1233';


UPDATE attendance
SET status='Present'
WHERE Stu_id='TG_2022_1408' and Course_code='TCS1233' or course_code='ICT1253'; 

